{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:prettier/recommended",
        "eslint:recommended"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true, "endOfLine": "auto" }],
        "no-prototype-builtins": 0,
        "no-console": "off",
        "no-unused-vars": 0,
        "no-dupe-keys": 0,
        "no-case-declarations": 0,
        "no-inner-declarations": 0,
        "linebreak-style": "off"
    }
}

